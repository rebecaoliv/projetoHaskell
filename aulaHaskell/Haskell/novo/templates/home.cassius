html
  height:calc(100vh);
  font-size: 0.9rem;

body
  background-color: #ededed;

li 
  display:inline;

ul
  list-style-type:none;

a, a:hover
  text-decoration: none;

.menuLeft
  width: 12%!important;
  background-color:#272e38;

.menuTop
  height: 40px;
  background-color: #2d3540;

.btnSair
  width: 50px; 
  margin-right: 15px;

.btnSair:hover, .nav-item:hover
  background-color: #575e66;
  transition: 0.3s;
